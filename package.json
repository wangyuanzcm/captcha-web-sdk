{
  "name": "captcha-web-sdk",
  "version": "1.0.0",
  "description": "A drag captcha SDK supporting both npm package and CDN usage",
  "main": "dist/cjs/tac.cjs.js",
  "module": "dist/esm/tac.esm.js",
  "browser": "dist/umd/tac.min.js",
  "unpkg": "dist/umd/tac.min.js",
  "jsdelivr": "dist/umd/tac.min.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/tac.esm.js",
      "require": "./dist/cjs/tac.cjs.js",
      "default": "./dist/esm/tac.esm.js"
    },
    "./dist/tac/css/tac.css": "./dist/tac/css/tac.css"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10",
  "directories": {
    "example": "public"
  },
  "examples": [
    "public/demo-cdn.html"
  ],
  "files": [
    "dist",
    "types",
    "readme.md",
    "LICENSE"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "scripts": {
    "start": "webpack serve --open --mode development",
    "build": "webpack --mode development",
    "buildprod": "webpack --mode production --progress",
    "clean:dist": "powershell -NoProfile -Command \"if (Test-Path 'dist') { Remove-Item -Recurse -Force 'dist' }\"",
    "build:lib:umd:dev": "webpack --config webpack.config.lib.js --env format=umd --mode development",
    "build:lib:umd": "webpack --config webpack.config.lib.js --env format=umd --mode production",
    "build:lib:esm": "webpack --config webpack.config.lib.js --env format=esm --mode production",
    "build:lib:cjs": "webpack --config webpack.config.lib.js --env format=cjs --mode production",
    "build:lib": "npm run clean:dist && npm run build:lib:umd:dev && npm run build:lib:umd && npm run build:lib:esm && npm run build:lib:cjs",
    "prepublishOnly": "npm run build:lib",
    "release": "npm run build:lib"
  },
  "keywords": [
    "captcha",
    "drag",
    "slider",
    "sdk",
    "tac"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "javascript-obfuscator": "^4.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "sass": "^1.93.2",
    "sass-embedded": "^1.0.0",
    "sass-loader": "^15.0.0",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.10",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
